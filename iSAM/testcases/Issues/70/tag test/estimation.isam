@minimiser gammadiff
type numerical_differences
tolerance 0.0002
iterations 1000
step_size 1e-7
covariance f
active f

@penalty event_mortality_penalty
type process
log_scale True
multiplier 10

@penalty tagging_penalty
type process
log_scale F
multiplier 10


@estimate
parameter process[Recruitment].r0
lower_bound 1e3
upper_bound 1e8
type uniform_log

@estimate
parameter selectivity[ShelfselMale].mu
lower_bound 1
upper_bound 50
type uniform

@estimate
parameter selectivity[ShelfselMale].sigma_l
lower_bound 1
upper_bound 50
type uniform

@estimate
parameter selectivity[ShelfselMale].sigma_r
lower_bound 1
upper_bound 500
type uniform

@estimate
parameter selectivity[ShelfselMale].alpha
lower_bound 1
upper_bound 1
type uniform

