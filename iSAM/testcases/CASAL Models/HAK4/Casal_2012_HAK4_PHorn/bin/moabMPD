#!/bin/bash

WD="$(pwd)"
RUN="~/bin/casal -e -q -O MPD.dat "
echo $RUN > RUN
CMD="msub -l nodes=1 -l walltime=1:00:00 -l partition=slurm -l qos=standby -p -1000 -q large -o estimate.log.%j -e estimate.err.%j -S /bin/bash -V RUN"
echo "Running CASAL on MOAB in" $WD
echo -n "Job started at: " ; date
echo $RUN
COMMAND="cd $WD ; $CMD"
ssh horn@turbine $COMMAND

