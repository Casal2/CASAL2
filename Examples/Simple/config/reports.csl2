#####################################
# The Report definition for the model
#####################################

@report summary
type estimate_value

@report profile
type profile

@report objective
type objective_function

# Print default reports
@report default
type default
catchabilities true
derived_quantities true
observations true
projects true
processes true
selectivities true
parameter_transformations true


@report Init
type initialisation_partition

#MCMC objectives
@report objectives
type MCMC_Objective
file_name objectives
write_mode incremental_suffix

#MCMC samples
@report samples
type mcmc_sample
file_name samples
write_mode incremental_suffix

@report Hessian
type hessian_matrix

@report MCMCcovariance
type mcmc_covariance

