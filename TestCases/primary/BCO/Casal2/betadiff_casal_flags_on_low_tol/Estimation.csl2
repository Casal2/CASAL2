## Estimation script for HAK base model
#############
## Estimation BCO
#############
@minimiser betadiff
type betadiff
evaluations 400000
iterations 100000
tolerance 0.0000001

################
## MCMC
################
@mcmc mcmc
start 0
length 4000000
keep 2500
step_size 0.02
adapt_stepsize_at 10000 50000 150000 250000 450000 750000 1400000
proposal_distribution t
adapt_stepsize_method double_half

#############
## Estimates
#############
## B0
@estimate B0
type uniform_log
parameter process[Recruitment].b0
lower_bound 100
upper_bound 250000


@estimate male_Linf
type uniform
lower_bound 30
upper_bound 70
parameter age_length[asMm0].linf

@estimate male_cv2
type uniform
lower_bound 0.02
upper_bound 1.0
parameter age_length[asMm0].cv_last

@estimate potSurvey_mA50
type uniform
parameter selectivity[potSurveySel_male].a50
lower_bound 1
upper_bound 7

@estimate potSurvey_mAto95
type uniform
parameter selectivity[potSurveySel_male].ato95
lower_bound .1
upper_bound 5


@estimate potSurvey_fA50
type uniform
parameter selectivity[potSurveySel_female].a50
lower_bound 1
upper_bound 7

@estimate potSurvey_fAto95
type uniform
parameter selectivity[potSurveySel_female].ato95
lower_bound .1
upper_bound 5
