@minimiser betadiff
type betadiff
active true
iterations 500
evaluations 1000
tolerance 0.002
covariance true


@mcmc mcmc
start 0.2
length 11000000
keep 1000
step_size 0.02
adapt_stepsize_at 10000 20000 30000 40000 50000 100000 200000 4000000
proposal_distribution t
adapt_stepsize_method double_half
max_correlation 0.8


@estimate B0
parameter process[Recruitment].b0
type uniform_log
lower_bound 1e4
upper_bound 1e5


@estimate age_len_cv_first
parameter age_length[age_len_label].cv_first
type normal_by_stdev
lower_bound 0.01
upper_bound 1.0
mu 0.15
sigma 0.025
estimation_phase 3

@estimate age_len_cv_last
parameter age_length[age_len_label].cv_last
type normal
lower_bound 0.01
upper_bound 1.0
mu 0.1
cv 0.5
estimation_phase 3


