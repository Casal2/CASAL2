#parameter catchability[Qandes].q
#type uniform_log
#lower_bound 1e-10
#upper_bound 1e7


@estimate fishery_sel_a50
parameter selectivity[SELandes].a50
type normal
lower_bound  5
upper_bound 80
mu 35
cv 0.3
estimation_phase 2


@estimate fishery_sel_ato95
parameter selectivity[SELandes].ato95
type normal
lower_bound  1
upper_bound 20
mu 5
cv 0.5
estimation_phase 2


