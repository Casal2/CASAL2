## Estimation script for LIN3&4 base model (2014 assessment)
@minimiser ADOLC
type adolc
active true
iterations 500
evaluations 1000
tolerance 0.002
covariance true


@mcmc mcmc
start 0.3
length 30000000
keep 2500
step_size 0.02
adapt_stepsize_at 10000 50000 150000 250000 450000
# burn-in? burn_in 200
proposal_distribution t
adapt_stepsize_method double_half
max_correlation 0.9


@estimate B0
type uniform_log
parameter process[Recruitment].b0
lower_bound  50000
upper_bound 800000


@estimate summerTANq
type lognormal
parameter catchability[summerTANq].q
lower_bound 0.02
upper_bound 0.30
mu 0.13
cv 0.70

@estimate autumnTANq
type lognormal
parameter catchability[autumnTANq].q
lower_bound 0.02
upper_bound 0.30
mu 0.13
cv 0.70


## Natural Mortality
# male+female
@estimate M_all_x0
parameter selectivity[M_all].x0
type uniform
lower_bound 3
upper_bound 15

@estimate M_all_y0
parameter selectivity[M_all].y0
type uniform
lower_bound 0.01
upper_bound 0.6

@estimate M_all_y1
parameter selectivity[M_all].y1
type uniform
lower_bound 0.01
upper_bound 1.0

@estimate M_all_y2
parameter selectivity[M_all].y2
type uniform
lower_bound 0.01
upper_bound 1.0


