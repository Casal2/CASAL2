## Derived Quantities
## SSB which gets fed into Recruitment
@derived_quantity SSB
type biomass
time_step Sep_Nov
categories male female
time_step_proportion 0.5
time_step_proportion_method weighted_sum
selectivities MaturationSel_male MaturationSel_female


## SELECTIVITIES
@selectivity MaturationSel_male
type all_values_bounded
l 3
h 10
v 0 0 0.10 0.30 0.50 0.80 1.00 1.00

@selectivity MaturationSel_female
type all_values_bounded
l 3
h 10
v 0 0 0.05 0.10 0.30 0.50 0.80 1.00


@selectivity summerTANSel_m # Summer research survey
type logistic
a50 5
ato95  3
alpha  0.9

@selectivity summerTANSel_f # Summer survey
type logistic
a50 5
ato95  3
alpha  1


@selectivity autumnTANSel_m # Autumn survey
type logistic
a50 5
ato95  3
alpha  0.9

@selectivity autumnTANSel_f # Autumn survey
type logistic
a50 5
ato95  3
alpha  1


@selectivity trwlFSel_m # Trawl fishery
type double_normal
mu 10
sigma_l 3
sigma_r 25
alpha  1

@selectivity trwlFSel_f # Trawl fishery
type double_normal
mu 10
sigma_l 3
sigma_r 25
alpha  1


@selectivity lineHomeFSel_m # Line Non-spawning fishery
type logistic
a50 11
ato95  3
alpha  0.3

@selectivity lineHomeFSel_f # Line Non-spawning fishery
type logistic
a50 11
ato95  3
alpha  1


@selectivity lineSpawnFSel_m # Line Spawn fishery
type logistic
a50 11
ato95  3
alpha  0.5

@selectivity lineSpawnFSel_f # Line Spawn fishery
type logistic
a50 11
ato95  3
alpha  1


## Natural Mortality by age
@selectivity M_all
type double_exponential
x0 8
x1 3
x2 25
y0 0.18
y1 0.25
y2 0.5
alpha 1.0


# SIZE AT AGE
@age_length vb_male
type von_bertalanffy
casal_switch false
by_length true
time_step_proportions 0.4 0.0
k 0.188
t0 -0.67
Linf 93.2
cv_first 0.12
cv_last 0.12
distribution normal
length_weight wgt_male

@age_length vb_female
type von_bertalanffy
casal_switch false
by_length true
time_step_proportions 0.4 0.0
k 0.124
t0 -1.26
Linf 115.4
cv_first 0.12
cv_last 0.12
distribution normal
length_weight wgt_female


# SIZE-WEIGHT
@length_weight wgt_male
type basic
units tonnes
a 2.08e-9
b 3.190

@length_weight wgt_female
type basic
units tonnes
a 1.28e-9
b 3.303
