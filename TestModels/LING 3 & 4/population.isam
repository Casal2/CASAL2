## Derived Quantities
## SSB which gets fed into Recruitment
@derived_quantity SSB
type biomass
time_step step2
categories male female
time_step_proportion 0.5
time_step_proportion_method weighted_sum
selectivities MaturationSel_male MaturationSel_female

# SELECTIVITIES
@selectivity MaturationSel_male
type all_values_bounded
l 3
h 10
v 0 0 0.100 0.20 0.35 0.50 0.80 1.00

@selectivity MaturationSel_female
type all_values_bounded
l 3
h 10
v 0 0 0.001 0.10 0.20 0.35 0.50 1.00

@selectivity chatTANSel_m #January survey
type double_normal
mu 4.92260166 
sigma_l 1.50131777 
sigma_r 199.999999 
alpha 1.0

@selectivity chatTANSel_f #January survey
type double_normal
mu 9
sigma_l 3
sigma_r 10
alpha 1.0

@selectivity trwlFSel_m #Trawl fishery
type double_normal
mu 9
sigma_l 3
sigma_r 10
alpha 1.0

@selectivity trwlFSel_f #Trawl fishery
type double_normal
mu 9
sigma_l 3
sigma_r 10
alpha 1.0


@selectivity lineFSel #Line fishery
type logistic
a50 11.1
ato95  3.4

@selectivity One
type constant 
c 1

#SIZE AT AGE
@age_length vb_male
type von_bertalanffy
by_length true
time_step_proportions 0.5 0.0
k 0.127
t0 -0.70
Linf 113.9
cv_first 0.10
distribution normal
length_weight wgt_male 

@age_length vb_female
type von_bertalanffy
by_length true
time_step_proportions 0.5 0.0
k 0.083
t0 -0.74
Linf 156.4
cv_first 0.1 
distribution normal
length_weight wgt_female 


# SIZE-WEIGHT
#
@length_weight wgt_male
type basic
units tonnes 
a 1.00e-9
b 3.354

@length_weight wgt_female
type basic
units tonnes 
a 1.14e-9
b 3.318
#verify_size_weight 70 0.8 4

## Catchability
@catchability chatTANq
type free
q 0.0867659185

@assert obj
type objective_function
value 1782.72

