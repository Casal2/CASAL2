@age_length AgeLengthM
type von_bertalanffy
length_weight LengthWeightM
k     0.101
t0   -0.292
linf 164.06
cv_first 0.101
cv_last  0.101
time_step_proportions 0 0 0 0
by_length TRUE
distribution normal
compatibility_option casal

@age_length AgeLengthF
type von_bertalanffy
length_weight LengthWeightF
k     0.082
t0   -0.712
linf 180.49
cv_first 0.101
cv_last  0.101
time_step_proportions 0 0 0 0
by_length TRUE
distribution normal
compatibility_option casal

@length_weight LengthWeightM
type basic
units tonnes
a 1.247e-008
b 2.990

@length_weight LengthWeightF
type basic
units tonnes
a 7.361e-9
b 3.105

@age_length AgeLengthMTag
type von_bertalanffy
length_weight LengthWeightM
k     0.101
t0    0.208 #-0.292+0.5
linf 164.06
cv_first 0.101
cv_last  0.101
time_step_proportions 0 0 0 0
by_length TRUE
