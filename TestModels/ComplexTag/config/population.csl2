k     0.082
t0   -0.212 #-0.712+0.5
linf 180.49
cv_first 0.101
cv_last  0.101
time_step_proportions 0 0 0 0
by_length TRUE
distribution normal
compatibility_option casal

@process TagLoss2001
type tag_loss
categories TagYear=2001
tag_loss_rate 0.039
time_step_proportions 0.5 0.5
selectivities One
tag_loss_type single
year 2001

@process TagLoss2002
type tag_loss
categories TagYear=2002
tag_loss_rate 0.039
time_step_proportions 0.5 0.5
selectivities One
tag_loss_type single
year 2002

@process TagLoss2003
type tag_loss
categories TagYear=2003
tag_loss_rate 0.029
time_step_proportions 0.5 0.5
selectivities One
tag_loss_type single
year 2003

@process TagLoss2004Plus
type tag_loss
categories TagYear=2004:2020
tag_loss_rate 0.0084
time_step_proportions 0.5 0.5
selectivities One
tag_loss_type single
year 2004
