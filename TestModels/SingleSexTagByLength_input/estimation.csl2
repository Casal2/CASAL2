
@minimiser gammadiff
type betadiff
tolerance 0.0002
iterations 1000
#step_size 1e-7
covariance f
active f

@mcmc mcmc
start 0
length 4000000
keep 2500
step_size 0.02
adapt_stepsize_at 10000 50000 150000 250000 450000 750000 1400000
proposal_distribution t
adapt_stepsize_method double_half

@penalty event_mortality_penalty
type process
log_scale True
multiplier 10

@penalty tagging_penalty
type process
log_scale F
multiplier 10

