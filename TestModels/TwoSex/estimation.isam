## Estimation script for LIN3&4 base model (2014 assessment)
@minimiser gammdiff
type betadiff
tolerance 0.00002
iterations 1000

@mcmc mcmc
start 0
length 6000000
keep 1000
step_size 0.001
adapt_stepsize_at 10000 50000 100000 150000 
proposal_distribution t
adapt_stepsize_method double_half


@estimate B0
type uniform_log
parameter process[Recruitment].b0
lower_bound  30000
upper_bound 500000

@estimate chatTANq
type lognormal
parameter catchability[chatTANq].q
lower_bound 0.02
upper_bound 0.30
mu 0.13
cv 0.70

## Natural Mortality
@estimate
type uniform
parameter process[instant_mort].m(male)
same process[instant_mort].m(female)
lower_bound 0.01
upper_bound 0.6


## female chat TAN survey selectivity
@estimate 
parameter selectivity[chatTANSel_f].mu
lower_bound 1  
upper_bound 20 
type uniform

@estimate 
parameter selectivity[chatTANSel_f].sigma_l
lower_bound  1   
upper_bound 50 
type uniform

@estimate 
parameter selectivity[chatTANSel_f].sigma_r
lower_bound  1   
upper_bound 200 
type uniform
## Male chat TAN survey selectivity
@estimate 
parameter selectivity[chatTANSel_m].mu
lower_bound 1  
upper_bound 20 
type uniform

@estimate 
parameter selectivity[chatTANSel_m].sigma_l
lower_bound  1   
upper_bound 50 
type uniform

@estimate 
parameter selectivity[chatTANSel_m].sigma_r
lower_bound  1   
upper_bound 200 
type uniform

## female chat trawl selectivity
@estimate 
parameter selectivity[trwlFSel_f].mu
lower_bound 1  
upper_bound 20 
type uniform

@estimate 
parameter selectivity[trwlFSel_f].sigma_l
lower_bound  1   
upper_bound 50 
type uniform

@estimate 
parameter selectivity[trwlFSel_f].sigma_r
lower_bound  1   
upper_bound 200 
type uniform

## Male chat trawl selectivity
@estimate 
parameter selectivity[trwlFSel_m].mu
lower_bound 1  
upper_bound 20 
type uniform

@estimate 
parameter selectivity[trwlFSel_m].sigma_l
lower_bound  1   
upper_bound 50 
type uniform

@estimate 
parameter selectivity[trwlFSel_m].sigma_r
lower_bound  1   
upper_bound 200 
type uniform


## Line selectivity. force males = females
@estimate lineSel
parameter selectivity[lineFSel].a50
lower_bound 1  
upper_bound 20 
type uniform

@estimate lineFSel_f
parameter selectivity[lineFSel].ato95
lower_bound  1   
upper_bound 200 
type uniform



